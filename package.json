{
	"name": "@dulysse1/ts-helper",
	"version": "1.3.15",
	"description": "Typescript library that provide typing utilities âœ¨",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=22",
		"pnpm": ">=9"
	},
	"scripts": {
		"build": "tsup",
		"format": "prettier . --write",
		"lint": "npx eslint --fix .",
		"prepublishOnly": "pnpm build",
		"prepare": "husky",
		"check-exports": "attw --pack . --ignore-rules cjs-resolves-to-esm",
		"test": "npx tsc --extendedDiagnostics"
	},
	"keywords": [
		"node",
		"typescript",
		"type",
		"types",
		"safe",
		"tools",
		"typesafe",
		"utility",
		"helpers",
		"ts-helper"
	],
	"author": {
		"name": "Ulysse Dupont",
		"url": "https://github.com/dulysse",
		"email": "ulyssedupont2707@gmail.com"
	},
	"repository": {
		"url": "https://github.com/Dulysse/ts-helper",
		"type": "public"
	},
	"bugs": {
		"url": "https://github.com/Dulysse/ts-helper/issues"
	},
	"type": "module",
	"homepage": "https://github.com/Dulysse/ts-helper/blob/master/README.md",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"main": "./dist/index.js",
	"private": false,
	"license": "ISC",
	"devDependencies": {
		"@arethetypeswrong/cli": "0.18.2",
		"@eslint/js": "9.39.1",
		"eslint": "9.39.1",
		"eslint-plugin-prettier": "5.5.4",
		"globals": "16.5.0",
		"husky": "9.1.7",
		"prettier": "3.6.2",
		"tsup": "8.5.1",
		"typescript": "5.9.3",
		"typescript-eslint": "8.47.0"
	}
}
