{
	"name": "@dulysse1/ts-helper",
	"version": "1.0.0",
	"description": "Typescript library for typing helpers âœ¨",
	"main": "dist/index.mjs",
	"types": "dist/index.d.mts",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=18",
		"pnpm": ">=8"
	},
	"scripts": {
		"build": "tsup",
		"format:check": "prettier . --check",
		"format:fix": "prettier . --write",
		"lint": "npx eslint --fix .",
		"prepublishOnly": "pnpm build",
		"prepare": "husky install"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"keywords": [
		"node",
		"typescript",
		"type",
		"types",
		"safe",
		"tools",
		"typesafe"
	],
	"author": {
		"name": "Ulysse Dupont",
		"url": "https://github.com/dulysse"
	},
	"repository": {
		"url": "https://github.com/Dulysse/ts-helper",
		"type": "public"
	},
	"private": true,
	"license": "ISC",
	"devDependencies": {
		"@types/node": "^20.10.3",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.0.1",
		"husky": "^8.0.3",
		"prettier": "^3.1.0",
		"tsup": "^8.0.1",
		"typescript": "^5.3.2"
	}
}
